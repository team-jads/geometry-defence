<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:showIn="@layout/activity_main">

    <TextView
        android:id="@+id/title_2"
        android:layout_width="223dp"
        android:layout_height="68dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:focusable="false"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:inputType="none"
        android:text="DEFENSE"
        android:textAlignment="center"
        android:textColor="#C3D6CFCF"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_1" />

    <TextView
        android:id="@+id/title_1"
        android:layout_width="277dp"
        android:layout_height="81dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:focusable="false"
        android:fontFamily="@font/archivo_black"
        android:gravity="center"
        android:inputType="none"
        android:selectAllOnFocus="false"
        android:singleLine="false"
        android:text="GEOMETRY"
        android:textAlignment="center"
        android:textColor="#F3F1F1"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="34dp"
        android:layout_height="83dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/title_1"
        app:layout_constraintEnd_toEndOf="@+id/title_1"
        app:layout_constraintHorizontal_bias="0.683"
        app:layout_constraintStart_toStartOf="@+id/title_1"
        app:layout_constraintTop_toTopOf="@+id/title_1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/tower_icon" />
</merge>